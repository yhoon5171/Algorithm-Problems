#include <iostream>
#include <queue>
using namespace std;

int main()
{
    queue<int> cardbox;
    int N;

    cin >> N;

    for (int i = 1; i < N + 1; i++){
        cardbox.push(i);
    }

    while(cardbox.size() != 1){
        cardbox.pop();
        cardbox.push(cardbox.front());
        cardbox.pop();
    }
    
    cout << cardbox.back();
}
